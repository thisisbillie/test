git init
//global scope signature
git config --global user.name ""
git config --global user.email ""
//保存位置 ~/.gitconfig

//project scope signature
git config user.name ""
git config user.email ""
//保存位置 ./.git/config

git remote rm origin

vim nano

//check log
git log --pretty=oneline
git log --oneline
git reflog

//back and forth
git reset --hard ae90djke
git reset --hard HEAD^
git reset --hard HEAD~n
//hard: resets the index file and working tree
//mixed: resets the index file not working tree
//soft: do not touch the index file or working tree

git reset --mixed dkdh39
git reset --hard HEAD
//use this to sync the working tree(local file) with the index file

git diff
git diff 文件名  //comparing with the staging area
git diff HEAD 文件名  //comparing with historic versions

git branch -a
git branch 新分支 
git checkout 分支名  //切换到新分支

//合并分支
git checkout 主分支
git merge 要合并进来的分支名
//解决冲突：直接修改文本，然后add，然后不带文件名地commit


//SSH
rm -r .ssh
ssh-keygen -t rsa -C billie.hu.bingling@gmail.com
cd .ssh
ls
cat id_rsa.pub
//copy and paste the whole thing to your gitHub

//clone
git clone [ssh link]

